{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": "eastus",
            "metadata": {
                "description": "The location for the resources."
            }
        }
    },
    "resources": [
        {
            "apiVersion": "2018-09-01-preview",
            "type": "Microsoft.CustomProviders/resourceProviders",
            "name": "public",
            "location": "[parameters('location')]",
            "properties": {
                "actions": [
                    {
                        "name": "ProxyPost",
                        "routingType": "Proxy",
                        "endpoint": "https://incloud.duplocloud.net/InvokeAzureRpAtRoot"
                    },
                    {
                        "name": "VirtualMachines/ProxyPost",
                        "routingType": "Proxy",
                        "endpoint": "https://incloud.duplocloud.net/InvokeAzureRp"
                    },
                    {
                        "name": "VirtualMachines/startAzureHost",
                        "routingType": "Proxy",
                        "endpoint": "https://incloud.duplocloud.net/InvokeAzureRp"
                    },
                    {
                        "name": "VirtualMachines/stopAzureHost",
                        "routingType": "Proxy",
                        "endpoint": "https://incloud.duplocloud.net/InvokeAzureRp"
                    }
                ],
                "resourceTypes": [
                    {
                        "name": "VirtualMachines",
                        "routingType": "Proxy",
                        "endpoint": "https://incloud.duplocloud.net/InvokeAzureRp"
                    },
                    {
                        "name": "UpdatePolicies",
                        "routingType": "Proxy",
                        "endpoint": "https://incloud.duplocloud.net/InvokeAzureRp"
                    },
                    {
                        "name": "BackupPolices",
                        "routingType": "Proxy",
                        "endpoint": "https://incloud.duplocloud.net/InvokeAzureRp"
                    }
                ]
            }
        }
    ],
    "outputs": {
    }
  } 